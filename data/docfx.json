{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "**/*.csproj"
          ],
          "exclude": [
            "**/.vs/**",
            "**/obj/**",
            "**/bin/**",
            "**/App_Data/**",
            "**/modules/**",
            "**/imports/**",

            "**/node_modules/**",
            "**/dist/**",

            "**/css/**",
            "**/styles/**",
            "**/images/**",
            "**/static/**",
            
            "**/*.Tests.csproj",
            "**/*.Benchmarks.csproj"
          ]
        }
      ],
      "dest": "[%projectDirectory%][%projectName%]",
      "disableGitFeatures": false,
      "disableDefaultFilter": false,
      "filter": "[%projectName%]_filter.yml"
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "[%projectDirectory%][%projectName%]/**.yml",
          "[%projectName%]/**.yml",
          "[%projectName%]_index.md",
          "index.md"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "**/index.md"
        ],
        "exclude": [
          "obj/**",
          "bin/**",
          "App_Data/**",
          "imports/**",
          "properties/**",
          "**/*.Tests.csproj",
          "**/*.Benchmarks.csproj"
        ]
      }
    ],
    "dest": "[%projectDirectory%][%projectName%]_site",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "docfx_custom_template"
    ],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
  }
}